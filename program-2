# TO FIND ELEMENTS IN DIFFERENT LOCATIONS
n=int(input())
l=list(map(int,input().split()))
x=int(input())
flag=0
for i in range(0,n):
    if l[i]==x:
        flag=1
        if flag==1:
            print(i,end=' ')
if flag==0:
    print('-1')

